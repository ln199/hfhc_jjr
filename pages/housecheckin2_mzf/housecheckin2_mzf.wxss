/* pages/housecheckin2/housecheckin2.wxss */
.graoupa{
  width: 100%;
  height: auto;
  margin: auto;
}
.title{
  font-size: 40rpx;
  font-weight: bold;
  margin-left: 23rpx;
  padding-top: 41rpx;
  padding-bottom: 36rpx;
}
.line{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 1rpx solid #F2F2F2;
  height: 119rpx;
  margin: auto 40rpx;
}
.lineleft{
  font-size: 32rpx;
  font-weight: bold;
  width: 160rpx;
  text-align: right;
}
.lineleft text{
  margin-right: 8rpx;
}
.input-text{
  width: 70%;
  font-size: 32rpx;
  font-weight: 500;
  padding-left: 41rpx;
}
radio{
  margin-left: 39rpx;
}
radio .wx-radio-input{
  width: 25rpx;
  height: 25rpx;
  border-radius: 50%;
}
radio .wx-radio-input.wx-radio-input-checked{
  width: 25rpx;
  height: 25rpx;
  border: 2rpx solid #EA5413 !important;
  background: #EA5413 !important;
}
radio .wx-radio-input.wx-radio-input-checked::before{
  border-radius: 50%;
  text-align: center;
  color: #fff;
  font-size: 20rpx;
  transform:translate(-50%, -50%) scale(1);
  -webkit-transform:translate(-50%, -50%) scale(1);
}
.radio text{
  font-size: 32rpx;
  font-weight: 500;
}
.arrow{
  width: 12rpx;
  height: 19rpx;
  display: block;
}
.picker{
  font-size: 32rpx;
  font-weight: 500;
}
.lineright{
  width: 70%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding-left: 41rpx;
  font-size: 32rpx;
  font-weight: 500;
}
.fg{
  width: 100%;
  height: 7rpx;
  margin: auto;
}
.input-texts{
  width: 132rpx;
  text-align: center;
}
.lineb{
  border-bottom: 1rpx solid #F2F2F2;
  height: auto;
  margin: auto;
  width: 93%;
  padding-bottom: 12rpx;
}
.lineb_left{
  font-size: 32rpx;
  font-weight: bold;
  width: 172rpx;
  text-align: right;
  line-height: 117rpx;
}
.biaozu{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.box-container{
  width:200rpx;
  height:56rpx;
  border-radius:8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #F2F2F2;
  text-align: center;
  margin: 0 17.5rpx 20rpx 17.5rpx;
}
.left{
  font-size: 24rpx;
  font-weight: 500;
}
._left{
  font-size: 24rpx;
  font-weight: 500;
  color: white;
  background-color: #EA5413;
}
.box-container view{
  width:200rpx;
  height:56rpx;
  border-radius:8rpx;
  line-height: 56rpx;
}
.btnk{
  width: 100%;
  height: 245rpx;
  margin: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
.btnk button{
  width: 86.36%;
  height: 80rpx;
  border-radius: 80rpx;
  font-size: 32rpx;
  font-weight: 500;
}