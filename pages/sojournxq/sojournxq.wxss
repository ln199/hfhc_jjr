.swiper-box {
  position: relative;
  width: 100%;
}

.dots {
  display: flex;
  justify-content: space-around;
  width: 300rpx;
  height: 60rpx;
  background: rgba(255, 255, 255, 0.75);
  border-radius: 30rpx;
  margin: auto;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 29rpx;
  font-size: 30rpx;
  font-weight: 500;
}

.dots .dot {
  width: 100rpx;
  height: 60rpx;
  background: transparent;
  border-radius: 60rpx;
  transition: all .6s;
  text-align: center;
  line-height: 60rpx;
}

.dots .dot.active {
  width: 100rpx;
  background: #EA5413;
  font-weight: bold;
  color: #fff;
}

