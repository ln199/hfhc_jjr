/* pages/rentlist/rentlist.wxss */
/* 搜索 */
.frame{
  width:644rpx;
  height:60rpx;
  background:#ffffff;
  border:2rpx solid rgba(159,159,159,1);
  border-radius:30rpx;
  margin: auto;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 39rpx;
}
.frame_zuo{
  width:27rpx;
  height:29rpx;
  display: block;
  margin: 15rpx 21rpx 16rpx 73rpx;
}
.frame_you{
  width: 75%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 27rpx;
  font-weight: 500;
  text-align: left;
}
/* 列表 */
.tab-scoller{
  width: 100%;
  margin: auto;
}
.listitem{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 23rpx;
  padding-bottom: 45rpx;
}
.itemleft{
  line-height: 48rpx;
  margin-left: 19rpx;
}
.hang{
  font-size: 27rpx;
  font-weight: 500;
}
.xq{
  width:140rpx;
  height:53rpx;
  border-radius:27rpx;
  font-size: 27rpx;
  font-weight: 500;
  padding: 0;
  margin: 0;
  line-height: 53rpx;
  margin-right: 23rpx;
}
.fg{
  width: 100%;
  height: 8rpx;
  margin: auto;
}
/* 悬浮按钮 */
.add{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  position: fixed;
  bottom: 55rpx;
  right: 15rpx;
  z-index: 2;
}
.add image{
  width: 100%;
  height: 100%;
  display: block;
  border-radius: 50%;
}