.expressRecord {
  width: 100%;
  margin: 0 auto;
  padding-top: 23rpx;
}
.expressRecord-single-close {
  width: 100%;
  height: 143rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}
.expressRecord-single-noReach-online-top-close {
  display: flex;
  flex-direction: column;
}
.dot-closing {
  width: 28rpx;
  height: 28rpx;
  border-radius: 50%;
  border: 4rpx solid #faddd1;
  margin-left: 20rpx;
  background-color:#fff;
  position: relative;
}
.yuaning{
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #EA5413;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -10rpx;
  margin-left: -10rpx;
  z-index: 1;
}
.dot-close {
  width: 28rpx;
  height: 28rpx;
  border-radius: 50%;
  border: 4rpx solid #f2f2f2;
  margin-left: 20rpx;
  background-color: #fff;
  position: relative;
}
.yuan{
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #f2f2f2;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -10rpx;
  margin-left: -10rpx;
  z-index: 1;
}
.online-bottom {
  width: 4rpx;
  height: 50rpx;
  background-color: #f2f2f2;
  margin-left: 35rpx;
}
.online-bottomimg {
  width: 4rpx;
  height: 50rpx;
  background-color: #faddd1;
  margin-left: 35rpx;
}
.expressRecord-text {
  width: 85%;
  margin-left: 20rpx;
}
.expressRecord-status {
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 23rpx;
}
.expressRecord-status-address {
  font-size: 32rpx;
  font-weight: 500;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 18rpx;
}
.btnk{
  width: 100%;
  text-align: center;
  position: fixed;
  bottom: 0;
}
.btnk button{
  width:648rpx;
  height:80rpx;
  border-radius:40rpx;
  font-size: 32rpx;
  font-weight: 500;
  margin-bottom: 81rpx;
}